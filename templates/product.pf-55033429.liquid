    <title>Image Slider</title>
    <link rel="stylesheet" href="style.css">
  	<style>
.wrapper {
  	margin: 0 auto;
    position: relative;
    height: 500px;
    width: 750px;
    background: #fff;
    border: 7px solid #fff;
    box-shadow: 0px 0px 15px rgba(0,0,0,0.15);
    overflow: hidden;
}
.wrapper .images{
    width: 100%;
    height: 100%;
    display: flex;
}
.wrapper .images .image-1{
    width: 100%;
    height: 100%;
    background: url("https://cdn.shopify.com/s/files/1/0578/5980/1246/files/B2.png?v=1633975487") no-repeat;
}
.wrapper .images .image-2{
    position: absolute;
    width: 50%;
    height: 100%;
    background: url("https://cdn.shopify.com/s/files/1/0578/5980/1246/files/B1.png?v=1633975487") no-repeat;
}
.wrapper .slider{
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 99;
}
.wrapper .slider input{
    width: 100%;
    outline: none;
    background: none;
    -webkit-appearance: none;
}
.slider input::-webkit-slider-thumb{
    height: 486px;
    width: 3px;
    background: none;
    -webkit-appearance: none;
    cursor: col-resize;
}
.slider .drag_line {
    width: 3px;
    height: 486px;
    position: absolute;
    left: 49.85%;
    pointer-events: none;
}
.slider .drag_line::before,
.slider .drag_line::after{
    position: absolute;
    content: "";
    width: 100%;
    height: 218px;
    background: #fff;
}
.slider .drag_line::before{
    top: 0;
}
.slider .drag_line::after{
    bottom: 0;
}
.slider .drag_line span{
    position: absolute;
    width: 50px;
    height: 50px;
    top: 50%;
    left: 50%;
    border: 3px solid #fff;
    border-radius: 50%;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%;
    transform:translate(-50%, -50%);
}
.slider .drag_line span::before,
.slider .drag_line span::after{
    position: absolute;
    content: "";
    top: 50%;
    border: 10px solid transparent;
    border-bottom-width: 0px;
    border-right-width: 0px;
    transform: translate(-50%, -50%) rotate(45deg);
}
.slider .drag_line span::before{
    left: 40%;
    border-left-color: #fff;
}
.slider .drag_line span::after{
    right: 20%;
    border-top-color: #fff;
  	</style>
</head>
<body>
    <div class="wrapper">
        <!-- image area  -->
        <div class="images">
            <div class="image-1"></div>
            <div class="image-2"></div>
        </div>
        <!-- image area  -->
        <!-- image comparison area -->
        <div class="slider">
            <div class="drag_line">
                <span></span>
            </div>
            <input type="range" min="0" max="100" value="50">
        </div>
        <!-- image comparison area -->
    </div>

    <script>
        const slider = document.querySelector(".slider input");
        const image = document.querySelector(".images .image-2");
        const dragLine = document.querySelector(".slider .drag_line");
        slider.oninput = ()=>{
            let sliderVal = slider.value;
            dragLine.style.left = sliderVal + "%";
            image.style.width = sliderVal + "%";
        }
    </script>
</body>
